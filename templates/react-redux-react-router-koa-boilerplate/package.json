{
  "name": "react-redux-react-router-express-boilerplate",
  "version": "1.0.0",
  "description": "react redux react-router babel+es6 express boilerplate",
  "bin": {
    "pm2": "./node_modules/pm2/bin/pm2"
  },
  "scripts": {
    "unfreeze": "aegis unfreeze",
    "freeze": "aegis freeze",
    "build": "aegis build -c --verbose",
    "start": "aegis start -c --node --dev",
    "test": "aegis test --verbose",
    "test:watch": "aegis test -w --verbose",
    "serve": "pm2 start pm2.json",
    "stop": "pm2 delete pm2.json",
    "reserve": "pm2 restart pm2.json",
    "pm2monit": "pm2 monit",
    "pm2logs": "pm2 logs",
    "pm2flush": "pm2 flush"
  },
  "keywords": [
    "react",
    "redux",
    "babel",
    "es6",
    "karma",
    "mocha",
    "chai",
    "webpack",
    "koa",
    "react-router"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "domready": "^1.0.8",
    "flow-interfaces": "^0.6.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-cookie": "^1.0.0",
    "koa-favicon": "^2.0.0",
    "koa-json": "^1.1.3",
    "koa-logger": "^1.3.0",
    "koa-mount": "^1.3.0",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "lodash": "^4.16.2",
    "pm2": "^2.0.18",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redbox-react": "^1.3.1",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "source-map-support": "^0.4.2",
    "webpack": "^1.13.2"
  }
}
